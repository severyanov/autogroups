{
    "name": "AutoGroups",
    "description": "Groups tabs by site name",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ]
}